{
  "expo": {
    "name": "Troubleshooting",
    "slug": "Troubleshooting",
    "version": "1.1.0",
    "orientation": "portrait",
    "icon": "./assets/iconT.png",
    "splash": {
      "image": "./assets/splashT.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    }, 
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "Troubleshooting.App"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "build": {
        "babel": {
          "include": [
            "react-router-native"
          ]
        }
      }
    }
  }
}
